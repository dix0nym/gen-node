{
  "name": "gen-node",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/safe.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . gen-node --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/safe.ico --prune=true --out=release-builds --version-string.ProductName=\"Gen-Node\"",
    "package-linux": "electron-packager . gen-node --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/safe.png --prune=true --out=release-builds",
    "release": "dotenv release-it"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "crypto-random-string": "^3.2.0",
    "electron-store": "^5.1.1",
